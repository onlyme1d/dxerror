RewriteEngine On
 
RewriteCond %{REQUEST_URI} ^(.+)//(.*)$
RewriteRule .* %1/%2 [R=301,L]

RewriteCond %{QUERY_STRING} ^homeType=newUser$
RewriteRule ^$ /pelayanan/jadwal-pelayanan/? [R=301,L]

<IfModule mod_rewrite.c>
RewriteBase /pelayanan/jadwal-pelayanan/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /pelayanan/jadwal-pelayanan/index.php [L]
</IfModule>
